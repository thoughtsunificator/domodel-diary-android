		"cordova-android": "^9.1.0",
		"cordova-browser": "^6.0.0",
		"cordova-plugin-whitelist": "^1.3.4",
		"cross-env": "^7.0.3",
		"electron": "^15.1.1",
		"electron-builder": "^22.11.7",
		"eslint": "^7.0.0",
		"http-server": "^13.0.2",
		"mocha": "^9.1.1",
		"postcss": "^8.3.6",
		"postcss-import": "^14.0.2",
		"puppeteer": "^10.2.0",
		"rollup": "^2.50.4",
		"rollup-plugin-copy": "^3.4.0",
